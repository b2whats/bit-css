{
  "$schema": "https://raw.githubusercontent.com/Polymer/tachometer/master/config.schema.json",
  "root": "../..",
  "sampleSize": 30,
  "timeout": 0,
  "autoSampleConditions": [
    "0ms"
  ],
  "benchmarks": [
    {
      "expand": [{
        "name": "search char iteration",
        "url": "search.html?test=iteration&token=*"
      },{
        "name": "search char regexp",
        "url": "search.html?test=regexp&token=*"
      },{
        "name": "search word iteration",
        "url": "search.html?test=iteration&token=BAR"
      },{
        "name": "search word regexp",
        "url": "search.html?test=regexp&token=BAR"
      },{
        "name": "search double word regexp",
        "url": "search.html?test=regexp&token=FOO|BAR"
      },{
        "name": "search a lot of word regexp",
        "url": "search.html?test=regexp&token=GET|BAZ|OLO|DOD|BUM|DAR|POP|PUSH|CHECK|FOO|BAR"
      }],
      "browser": {
        "name": "chrome",
        "headless": true
      }
    }
  ]
}